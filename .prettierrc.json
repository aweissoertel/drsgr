{
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "printWidth": 140,
    "bracketSpacing": true,
    "endOfLine": "crlf",
    "plugins": ["@trivago/prettier-plugin-sort-imports"],
    "importOrder": ["^react(-dom(.)*)?$", "<THIRD_PARTY_MODULES>", "^@core/(.*)$", "^@server/(.*)$", "^@ui/(.*)$", "^[./]"],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}
  